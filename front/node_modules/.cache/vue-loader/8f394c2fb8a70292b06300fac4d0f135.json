       IdeaItem : [\n                ],\n                nowTotalHeight: 0,\n                nowHeight:0,\n            }\n        },\n        methods: {\n            async showIdea(){\n                await axios.get('http://localhost:8080/idea', \n                {\n                    headers : {\n                        'Authorization' : token\n                    }\n                }).then(res =>{\n                    this.IdeaItem = res.data.data;\n                    // console.log(this.IdeaItem);\n                })\n                this.nowTotalHeight = this.$refs.boardContainer.clientHeight;\n\n                // console.log( this.nowTotalHeight);\n                // console.log( this.$refs.boardContainer);\n            },\n\n            onScroll(e) {\n              // console.log(e);\n              e\n                // console.log(this.$refs.boardContainer);\n                // console.dir(e.target.scrollingElement);\n                // this.nowHeight = e.target.scrollingElement.scrollTop;\n                // console.log(this.nowHeight);\n                // console.log( this.nowTotalHeight);\n            }\n        },\n        \n    }\n</script>\n<style >\n    #icon{\n        background-image: url('../../assets/logo.png');background-size: 100% 100%;\n    }\n    #ideaItem:hover {\n        background-color: aquamarine;\n    }\n</style>"]}]}