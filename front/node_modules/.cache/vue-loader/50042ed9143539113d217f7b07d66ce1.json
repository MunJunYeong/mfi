    <v-col cols='4'>\n        <div style=\"text-align: center;\">\n          <router-link to=\"/auth/findId\">아이디찾기</router-link> ㅣ\n          <router-link to=\"/auth/findPw\">비밀번호찾기</router-link> ㅣ \n          <router-link to=\"/auth/signUp\">회원가입</router-link>\n        </div>\n      </v-col>\n    </v-row>\n    <br><br>\n    <v-row justify=\"center\">\n      <v-col cols='4'>\n        <div style=\"text-align: center;\">© 2021 MetaphorForInvesting.com. All rights reserved.</div>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\n  import axios from 'axios';\n\n  export default {\n    name: 'SignIn',\n    \n    props : {\n      \n    },\n    data() {\n      return{\n        id : '',\n        pw : '',\n      }\n    },\n    methods: {\n      // login : () =>{\n      //   console.log(this.id);\n      // }\n      login(){\n        axios.post('http://localhost:8080/signIn', {\n          id : this.id,\n          pw : this.pw\n          }).then( res => {\n            if(res.data.message === \"wrong pw\"){\n              alert('잘못된 비밀번호입니다.');\n            }else if(res.data.message === \"not exist id\"){\n              alert('잘못된 아이디입니다');\n            }else if(res.data.token){\n              localStorage.setItem(\"accessToken\", res.data.token);\n              location.href='#/home';\n            }else if(res.data.message === 'no data') {\n              alert('아이디와 비밀번호를 제대로 입력해주세요.');\n            }else{\n              alert('통신 오류');\n            }\n          })\n        }\n      },\n\n\n  }\n</script>\n"]}]}