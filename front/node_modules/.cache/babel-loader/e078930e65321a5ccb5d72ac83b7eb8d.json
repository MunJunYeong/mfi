Idx\" \n                :subject=\"item.subject\" \n                :content=\"item.content\"\n                /> \n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n<script>\nimport axios from 'axios'\n    import IdeaItem from '../../components/IdeaItem.vue'\n    let token = localStorage.getItem('accessToken');\n    \n    export default {\n        name: 'Idea',\n        created() {\n            this.showIdea();\n            // let height = this.$refs.boardContainer.clientHeight;\n        },\n        components: {\n            IdeaItem\n        },\n        props: {\n\n        },\n        data() {\n            return {\n                modelIdea: ['모든 아이디어'],\n                selectIdea: [\n                  '모든 아이디어',\n                  '위너 아이디어'\n                ],\n                modelDate : ['최신 순'],\n                selectDate: [\n                  '최신 순',\n                  '오래된 순'\n                ],\n\n                IdeaItem : [\n                ],\n                nowTotalHeight: 0,\n                nowHeight:0,\n            }\n        },\n        methods: {\n            async showIdea(){\n                await axios.get('http://localhost:8080/idea', \n                {\n                    headers : {\n                        'Authorization' : token\n                    }\n                }).then(res =>{\n                    this.IdeaItem = res.data.data;\n                    // console.log(this.IdeaItem);\n                })\n                this.nowTotalHeight = this.$refs.boardContainer.clientHeight;\n\n                // console.log( this.nowTotalHeight);\n                // console.log( this.$refs.boardContainer);\n            },\n\n            onScroll(e) {\n              // console.log(e);\n              e\n                // console.log(this.$refs.boardContainer);\n                // console.dir(e.target.scrollingElement);\n                // this.nowHeight = e.target.scrollingElement.scrollTop;\n                // console.log(this.nowHeight);\n                // console.log( this.nowTotalHeight);\n            }\n        },\n        \n    }\n</script>\n<style >\n    #icon{\n        background-image: url('../../assets/logo.png');background-size: 100% 100%;\n    }\n    #ideaItem:hover {\n        background-color: aquamarine;\n    }\n</style>"],"sourceRoot":"src/views/core"}]}