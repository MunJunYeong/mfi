n<template>\n    <v-container>\n        <br>\n        <v-row justify='center'>\n            <v-col cols='3'  >\n                <div style=\" height: 75px; \" >\n                    <p style=\"font-size: 1.5em;\">모든아이디어</p> \n                    <p style=\"font-size: 0.8em;\">12,411개 게시물</p></div>\n            </v-col>\n            <!-- 여백 -->\n            <v-col cols='3'>\n                <div style=\"height: 75px;\"></div>\n            </v-col>\n\n            <v-col cols='1'  >\n                 <router-link to=\"/add-idea\">\n                    <v-btn block   style=\"margin-top: 10%;\" >\n                        아이디어 내기\n                    </v-btn>\n                </router-link>\n            </v-col>\n            <v-col cols='1'  >\n                <div id=\"icon\" style=\"height: 75px;\"></div>\n            </v-col>\n            <v-col cols='2'  >\n                <div style=\"height: 75px;\">\n                    <v-combobox\n                    v-model=\"modelIdea\"\n                    :items=\"selectIdea\" outlined\n                    >\n                    </v-combobox>\n                </div>\n            </v-col>\n\n            <v-col cols='1'  >\n                <div style=\"height: 75px;\">\n                    <v-combobox\n                    v-model=\"modelDate\"\n                    :items=\"selectDate\" outlined\n                    >\n                    </v-combobox>\n                </div>\n            </v-col>\n        </v-row>\n\n        <v-row justify='center'>\n            <v-col>\n                <!-- 공백 -->\n            </v-col>\n        </v-row>\n        \n        <v-row justify='center' >\n            <v-col cols='12'>\n                <IdeaList v-for=\"(item, index) in this.ideaList\"  :key=\"index\" \n                :author=\"item.author\" \n                :count=\"item.count\" \n                :subject=\"item.subject\" \n                :content=\"item.content\"\n                />\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n<script>\nimport axios from 'axios'\n    import IdeaList from './IdeaList.vue'\n    let token = localStorage.getItem('accessToken');\n    // let ideaList = [];\n    \n    export default {\n        name: 'Idea',\n        created() {\n            this.showIdea()\n        },\n        components: {\n            IdeaList\n        },\n        props: {\n\n        },\n        data() {\n            return {\n                modelIdea: ['모든 아이디어'],\n                selectIdea: [\n                  '모든 아이디어',\n                  '위너 아이디어'\n                ],\n                modelDate : ['최신 순'],\n                selectDate: [\n                  '최신 순',\n                  '오래된 순'\n                ],\n\n                ideaList : [\n                ],\n            }\n        },\n        methods: {\n            async showIdea(){\n                await axios.get('http://localhost:8080/idea', \n                {\n                    headers : {\n                        'Authorization' : token\n                    }\n                }).then(res =>{\n                    this.ideaList = res.data.data;\n                    console.log(this.ideaList)\n                })\n            }\n        },\n        \n    }\n</script>\n<style >\n    /* #a{\n        font-size: 1em; background-image: url('../assets/adjust.png');\n    } */\n    #icon{\n        background-image: url('../assets/adjust.png');background-size: 100% 100%;\n        \n    }\n</style>"],"sourceRoot":"src/components"}]}