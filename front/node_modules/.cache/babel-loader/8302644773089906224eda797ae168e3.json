GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,OAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,OAAA;AACA,iBAXA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA;AAdA;AAnBA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-row justify='center'>\n            <v-col cols='4'>\n                <div id=\"subject\">\n                    <h1>{{subject}}</h1>\n                    </div>\n            </v-col>\n            <v-spacer></v-spacer>\n            <v-col cols='1'>\n                <router-link to=\"/add-idea\">\n                    <v-btn style=\"height : 70px\"\n                    block outlined\n                    >아이디어 내기\n                    </v-btn>\n                </router-link>\n            </v-col>\n            \n        </v-row>\n\n        <v-row justify='center'>\n            <v-col cols='3'>\n                <div id=\"profile\"></div>\n                <div style=\"float : left; width: 7%; height: 100%;\"></div>\n                <div style=\"float : left; width: 73%; height: 100%;\">\n                    <p id=\"nickName\">닉네임 : {{nickName}}</p>\n                    <p id=\"created\">{{created}}</p>\n                </div>\n            </v-col>\n            <v-spacer></v-spacer>\n            <v-col cols='1'>\n                <v-btn id=\"modifyBtn\"\n                block outlined\n                >수정\n                </v-btn>\n                \n            </v-col>\n            <v-col cols='1'>\n                <v-btn id=\"deleteBtn\"\n                block outlined\n                >삭제\n                </v-btn>\n            </v-col>\n        </v-row>\n        \n        <!-- 내용 보이는 곳 -->\n        <v-row justify='center'>\n            <v-col cols='12'>\n                <p>{{content}}</p>\n            </v-col>\n        </v-row>\n        <br><br>\n        <hr>\n        <br><br>\n        <!-- 댓글적는곳 -->\n        <v-row justify='center'>\n            <v-col cols='2'>\n                <div style=\"background-color : yellow;\">아이콘</div>\n            </v-col>\n            <v-spacer></v-spacer>\n            <v-col cols='9' >\n                <div style=\"background-color : yellow;\">댓글 적기</div>\n            </v-col>\n        </v-row>\n    </v-container>\n    \n</template>\n\n<style>\n    #subject {\n         line-height: 70px;\n    }\n    #profile{\n        width: 20%; height: 100%; background-image: url('../../assets/profile.png' ); background-size: 100% 100%; float: left;\n        \n    }\n    #nickName {\n\n    }\n    #created {\n\n    }\n    #modifyBtn{\n        height: 50px;\n    }\n    #deleteBtn{\n        height: 50px;\n    }\n\n</style>\n\n\n<script>\nimport axios from 'axios';\nlet token = localStorage.getItem('accessToken');\n\n    export default {\n        name: 'IdeaClick',\n        created(){\n            this.showClickIdea();\n        },\n        mounted(){\n            \n        },\n        props: [\n            'ideaIdx',\n        ],\n        data(){\n            return {\n                subject : '',\n                nickName : '',\n                created : '',\n                content : '',\n            }\n        },\n        methods : {\n            async showClickIdea(){\n                await axios.get('http://localhost:8080/idea/idea-click?ideaIdx='+this.ideaIdx, \n                {\n                    headers : {\n                        'Authorization' : token\n                    }\n                }).then(res => {\n                    console.log(res.data.data[0].user)\n                    this.subject = res.data.data[0].subject;\n                    this.nickName = res.data.data[0].user.nickName;\n                    this.created = res.data.data[0].created;\n                    this.content = res.data.data[0].content\n                })\n            }\n        }\n\n    }\n\n\n</script>\n\n"],"sourceRoot":"src/views/core"}]}